{
- name: Setup Expo & EAS
  uses: expo/expo-github-action@v8
  with:
    expo-version: latest
    eas-version: latest
    token: ${{ secrets.EXPO_TOKEN }}

- name: Initialize EAS project
  run: npx eas init --non-interactive --force

- name: Build APK
  run: eas build -p android --profile preview --local --output=./amc-teams.apk

  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "production": {}
  }
}
